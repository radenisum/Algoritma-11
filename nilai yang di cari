#include <iostream>

using namespace std;

int main()
{
	int A [10];
	int i;
	int find=0;


	for (i=0; i<10; i++)
	{
	cout<< "Maukkan data ke " << i+1 <<" = ";
	cin>> A[i];

	}

	    cout<< "Nilai yang di cari : ";
	    cin>>find;
	    

	   for (i=0; i<10; i++)
	{
	    if (A[i] == find){
	    cout<< "nilai yang anda cari terdapat pada indeks ke- "<< i+1 <<endl;
	    break;}
	}
    
	return 0;
}
